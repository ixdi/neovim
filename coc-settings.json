{
  "npm.binPath": "/bin/npm",
  "coc.preferences.formatOnSaveFiletypes": [
    "Markdown",
    "css",
    "html",
    "javascript",
    "javascriptreact",
    "markdown",
    "typescript",
    "typescriptreact"
  ],
  "coc.source.around.priority": 1,
  "coc.source.buffer.priority": 2,
  "coc.source.file.priority": 3,
  "smartf.timeout":3000,
  "smartf.jumpOnTrigger":false,
  "eslint.autoFixOnSave": true,
  "eslint.enable": true,
  "eslint.packageManager": "npm",
  "eslint.run": "onType",
  "html.enable": false,
  "html.format.enable": true,
  "list.source.files.excludePatterns": ["**/node_modules/*", "**/.meteor/*", "**/packages/*"],
  "prettier.jsxSingleQuote": true,
  "prettier.requireConfig": true,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "all",
  "snippets.snipmate.enable": true,
  "suggest.minTriggerInputLength": 2,
  "suggest.snippetIndicator": "â–º",
  "suggest.triggerAfterInsertEnter": true,
  "yank.enableCompletion": false,
  "yank.highlight.enable": false,
  "yank.limit": 50,
  "list.source.tags.command": "ctags -R --options=.ctags",
  "css.validate":false,
  "less.validate": false,
  "scss.validate": false,
  "wxss.validate": false
}
