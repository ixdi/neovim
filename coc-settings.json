{
  "npm.binPath": "/bin/npm",
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "scss",
    "html",
    "json",
    "javascript",
    "javascriptreact",
    "markdown",
    "typescript",
    "typescriptreact"
  ],
  "coc.source.around.priority": 1,
  "coc.source.buffer.priority": 2,
  "coc.source.file.priority": 3,
  "list.source.grep.excludePatterns": ["**/node_modules/**", "ketcher", "*trajectory-server*", "**/public/**","**/.meteor/**", "**/externals/**"],
  "smartf.timeout":3000,
  "smartf.jumpOnTrigger":false,
  "eslint.alwaysShowStatus": true,
  "eslint.autoFixOnSave": true,
  "eslint.packageManager": "npm",
  "eslint.run": "onType",
  "html.enable": true,
  "html.format.enable": true,
  "json.enable": true,
  "json.format.enable": true,
  "list.source.files.excludePatterns": ["**/node_modules/*", "**/.meteor/*", "**/packages/*"],
  "markdownlint.config": {
    "line-length": false
  },
  "prettier.jsxSingleQuote": true,
  "prettier.requireConfig": true,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "all",
  "snippets.snipmate.enable": true,
  "suggest.minTriggerInputLength": 2,
  "suggest.snippetIndicator": "â–º",
  "suggest.triggerAfterInsertEnter": true,
  "yank.enableCompletion": false,
  "yank.highlight.enable": false,
  "yank.limit": 50,
  "list.source.tags.command": "ctags -R --options=.ctags",
  "css.validate": true,
  "less.validate": false,
  "scss.validate": true,
  "wxss.validate": false
}
