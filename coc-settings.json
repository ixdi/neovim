{
  "coc.preferences.formatOnSaveFiletypes": [
    "Markdown",
    "css",
    "html",
    "javascript",
    "javascriptreact",
    "markdown",
    "typescript",
    "typescriptreact"
  ],
  "coc.source.around.priority": 1,
  "coc.source.buffer.priority": 2,
  "coc.source.file.priority": 3,
  "emmet.optimizeStylesheetParsing": true,
  "emmet.showExpandedAbbreviation": "always",
  "eslint.autoFixOnSave": true,
  "eslint.enable": true,
  "html.enable": false,
  "html.format.enable": true,
  "list.source.files.excludePatterns": ["**/node_modules/*", "**/.meteor/*", "**/packages/*"],
  "prettier.jsxSingleQuote": true,
  "prettier.requireConfig": true,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "all",
  "snippets.snipmate.enable": true,
  "suggest.minTriggerInputLength": 2,
  "suggest.snippetIndicator": "â–º",
  "suggest.triggerAfterInsertEnter": true,
  "yank.enableCompletion": false,
  "yank.highlight.enable": false,
  "yank.limit": 50
}
